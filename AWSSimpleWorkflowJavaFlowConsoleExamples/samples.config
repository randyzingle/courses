####### SWF PROD ENDPOINT #######
service.url=http://swf.us-east-1.amazonaws.com

####### CREDENTIALS FOR SWF #######
AWS.Access.ID=<Your AWS Account AccessKey>
AWS.Secret.Key=<Your AWS Account SecretKey>
AWS.Account.ID=<Your AWS Account AccountID>

####### CREDENTIALS FOR S3 #######
S3.Access.ID=<Your AWS Account AccessKey>
S3.Secret.Key=<Your AWS Account SecretKey>
S3.Account.ID=<Your AWS Account AccountID>

######## COMMON CONFIG ###########
domain=<Amazon SWF Domain Name>

####### Image Processing Sample Configuration ##########
Activity.Worker.LocalFolder=/tmp/
Activity.Worker.CommonTaskList=imageProcessingSampleCommonTaskList
Workflow.Worker.Tasklist=imageProcessingSampleDecisionTaskList

Workflow.Execution.Id=testrun
Workflow.Input.SourceFileName=test_image.jpg
Workflow.Input.SourceBucketName=swfconsole
Workflow.Input.BucketName=<Your S3 Bucket Name>

# Valid Options are GRAY_SCLAE or SEPIA
Workflow.Input.ImageProcessingOption=SEPIA                                                                    